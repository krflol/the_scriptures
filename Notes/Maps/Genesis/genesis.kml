<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Genesis Locations and Regions</name>
    <description>A consolidated map of locations and regions from Genesis.</description>

    <!-- Styles -->
    <Style id="pushpinStyle">
      <IconStyle>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
        </Icon>
      </IconStyle>
    </Style>
    <Style id="shinarPolygonStyle">
      <LineStyle>
        <color>ff0077ff</color>
        <width>2</width>
      </LineStyle>
      <PolyStyle>
        <color>800077ff</color>
      </PolyStyle>
    </Style>
    <Style id="havilahPolygonStyle">
      <LineStyle>
        <color>ffffaa00</color>
        <width>2</width>
      </LineStyle>
      <PolyStyle>
        <color>80ffaa00</color>
      </PolyStyle>
    </Style>
    <Style id="cushPolygonStyle">
      <LineStyle>
        <color>ff00aaff</color>
        <width>2</width>
      </LineStyle>
      <PolyStyle>
        <color>8000aaff</color>
      </PolyStyle>
    </Style>
    <Style id="edenPolygonStyle">
      <LineStyle>
        <color>ff00ff00</color>
        <width>2</width>
      </LineStyle>
      <PolyStyle>
        <color>8000ff00</color>
      </PolyStyle>
    </Style>

