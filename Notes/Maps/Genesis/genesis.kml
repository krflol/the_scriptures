    <!-- Regions and Kingdoms -->
    <Folder>
        <name>Regions and Kingdoms</name>

        <!-- Land of Shinar -->
        <Placemark>
            <name>Land of Shinar (Approximate)</name>
            <styleUrl>#shinarPolygonStyle</styleUrl>
            <Polygon>
                <outerBoundaryIs>
                    <LinearRing>
                        <coordinates>
                            44.40007533685579,32.50025817205463
                            45.6289334,31.3179591
                            44.36781751855634,33.34809941102404
                            44.40007533685579,32.50025817205463
                        </coordinates>
                    </LinearRing>
                </outerBoundaryIs>
            </Polygon>
        </Placemark>

        <!-- Land of Havilah (North/Central Arabia) -->
        <Placemark>
            <name>Land of Havilah (North/Central Arabia - Wadi al-Batin)</name>
            <styleUrl>#havilahPolygonStyle</styleUrl>
            <Polygon>
                <outerBoundaryIs>
                    <LinearRing>
                        <coordinates>
                            43.0,29.0
                            46.0,29.0
                            46.0,26.0
                            43.0,26.0
                            43.0,29.0
                        </coordinates>
                    </LinearRing>
                </outerBoundaryIs>
            </Polygon>
        </Placemark>

        <!-- Land of Havilah (Southwestern Arabia) -->
        <Placemark>
            <name>Land of Havilah (Southwestern Arabia - Wadi Bishah)</name>
            <styleUrl>#havilahPolygonStyle</styleUrl>
            <Polygon>
                <outerBoundaryIs>
                    <LinearRing>
                        <coordinates>
                            42.0,20.0
                            45.0,20.0
                            45.0,17.0
                            42.0,17.0
                            42.0,20.0
                        </coordinates>
                    </LinearRing>
                </outerBoundaryIs>
            </Polygon>
        </Placemark>

        <!-- Land of Cush (African) -->
        <Placemark>
            <name>Land of Cush (African - Nubia/Ethiopia)</name>
            <styleUrl>#cushPolygonStyle</styleUrl>
            <Polygon>
                <outerBoundaryIs>
                    <LinearRing>
                        <coordinates>
                            30.0,24.0
                            35.0,24.0
                            35.0,15.0
                            30.0,15.0
                            30.0,24.0
                        </coordinates>
                    </LinearRing>
                </outerBoundaryIs>
            </Polygon>
        </Placemark>

        <!-- Land of Cush (Mesopotamian) -->
        <Placemark>
            <name>Land of Cush (Mesopotamian - Kassites)</name>
            <styleUrl>#cushPolygonStyle</styleUrl>
            <Polygon>
                <outerBoundaryIs>
                    <LinearRing>
                        <coordinates>
                            45.0,35.0
                            48.0,35.0
                            48.0,32.0
                            45.0,32.0
                            45.0,35.0
                        </coordinates>
                    </LinearRing>
                </outerBoundaryIs>
            </Polygon>
        </Placemark>

        <!-- Borders of the Canaanites -->
        <Placemark>
          <name>Approximate Canaanite Territory</name>
          <styleUrl>#territoryPolygonStyle</styleUrl>
          <Polygon>
            <tessellate>1</tessellate>
            <altitudeMode>clampToGround</altitudeMode>
            <outerBoundaryIs>
              <LinearRing>
                <coordinates>
                  35.3756,33.5586 <!-- Sidon -->
                  34.4667,31.5   <!-- Gaza -->
                  35.5300,31.2400 <!-- Cities of the Plain -->
                  35.6100,31.5800 <!-- Lasha -->
                  35.3756,33.5586 <!-- Sidon (to close polygon) -->
                </coordinates>
              </LinearRing>
            </outerBoundaryIs>
          </Polygon>
        </Placemark>

    </Folder>
